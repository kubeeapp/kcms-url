{
  "name": "kubeeapp/kcms-url",
  "type": "wordpress-muplugin",
  "license": "MIT",
  "description":
    "Fixes issues with home and site URL on multisite when using KCMS",
  "homepage": "https://github.com/kubeeapp/kcms-url",
  "keywords": ["wordpress"],
  "support": {
    "issues": "https://github.com/kubeeapp/kcms-url/issues",
    "forum": "https://discourse.kubee.com/"
  },
  "autoload": {
    "psr-4": {
      "Roots\\Bedrock\\": "src/"
    }
  },
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.1"
  },
  "scripts": {
    "test": ["phpcs --ignore=vendor --extensions=php --standard=PSR2 ."]
  }
}
